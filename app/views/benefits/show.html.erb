<div class="page-header">
  <h3>Benefit #<%= @benefit.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Monetaryamt</dt>
      <dd><%= @benefit.monetaryamt %></dd>

      <dt>Percent</dt>
      <dd><%= @benefit.percent %></dd>

      <dt>Covcat</dt>
      <dd><%= @benefit.covcat_id %></dd>

      <dt>Insplan</dt>
      <dd><%= @benefit.insplan_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/benefits" class="btn btn-primary">
        Back
      </a>
      <a href="/benefits/<%= @benefit.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_benefit/<%= @benefit.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
