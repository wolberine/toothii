<div class="page-header">
  <h1>
    Covcats
    <a href="/covcats/new" class="btn btn-lg btn-success">New Covcat</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Defaultpercent</th>
        <th>Description</th>
        <th>Actions</th>
      </tr>

      <% @covcats.each do |covcat| %>
      <tr>
        <td><%= covcat.defaultpercent %></td>
        <td><%= covcat.description %></td>
        <td>
          <a href="/covcats/<%= covcat.id %>" class="btn btn-primary">Show</a>
          <a href="/covcats/<%= covcat.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_covcat/<%= covcat.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
