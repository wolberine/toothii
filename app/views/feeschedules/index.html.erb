<div class="page-header">
  <h1>
    Feeschedules
    <a href="/feeschedules/new" class="btn btn-lg btn-success">New Feeschedule</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Description</th>
        <th>Actions</th>
      </tr>

      <% @feeschedules.each do |feeschedule| %>
      <tr>
        <td><%= feeschedule.description %></td>
        <td>
          <a href="/feeschedules/<%= feeschedule.id %>" class="btn btn-primary">Show</a>
          <a href="/feeschedules/<%= feeschedule.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_feeschedule/<%= feeschedule.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
